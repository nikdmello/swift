# Production Environment Variables
NODE_ENV=production
DOMAIN=swift-protocol.com
SSL_EMAIL=admin@swift-protocol.com

# Database
DATABASE_URL=postgresql://swift:${DB_PASSWORD}@postgres:5432/swift_production
